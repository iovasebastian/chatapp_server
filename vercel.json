{
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.7.0"
      }
    },
    "routes": [
      { "src": "/index.php", "dest": "/api/index.php" },
      { "src": "/addfriends.php", "dest": "/api/addfriends.php" },
      { "src": "/deletefriendship.php", "dest": "/api/deletefriendship.php" },
      { "src": "/getfriends.php", "dest": "/api/getfriends.php" },
      { "src": "/getmessages.php", "dest": "/api/getmessages.php" },
      { "src": "/sendmessage.php", "dest": "/api/sendmessage.php" },
      { "src": "/register.php", "dest": "/api/register.php" },
      { "src": "/js/script.js", "dest": "/js/script.js" }
    ]
  }
  